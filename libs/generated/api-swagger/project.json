{
  "name": "api-swagger",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "targets": {
    "build": {
      "executor": "nx:noop",
      "outputs": [
        "libs/generated/api-swagger"
      ]
    },
    "codegen": {
      "executor": "nx:run-commands",
      "options": {
        "command": "openapi-generator-cli generate -i libs/generated/api-swagger/swagger.json -g typescript-fetch -o libs/generated/api-types"
      },
      "dependsOn": [
        "swagger"
      ]
    }
  },
  "implicitDependencies": [
    "api"
  ]
}
